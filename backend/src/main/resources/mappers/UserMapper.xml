<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vu.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.vu.domain.User">
        <id column="id" property="id" />
        <result column="vunetid" property="vunetId" />
        <result column="password" property="password" />
        <result column="courses" property="courses" />
        <result column="email" property="email" />
        <result column="student_name" property="studentName" />
    </resultMap>

    <sql id="Base_Column_List">
        id, vunetid, password, courses, email, student_name
    </sql>

</mapper>
